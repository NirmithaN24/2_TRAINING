// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int x;
    int *p;
    int **d;
    x=69;
    p=&x;
    d=&p;
    printf("%d \n",x);
    printf("%d \n",&x);
    printf("%d \n",p);
    printf("%d \n",&p);
    printf("%d \n",*p);
    printf("%d \n",&d);
    printf("%d \n",*d);
    printf("%d \n",**d);
    return 0;
}
